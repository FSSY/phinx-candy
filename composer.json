{
  "name": "fssy/phinx-candy",
  "type": "library",
  "description": "A candy for phinx",
  "keywords": [
    "php",
    "phinx",
    "migrations",
    "database",
    "db",
    "database migrations"
  ],
  "authors": [
    {
      "name": "fssy",
      "email": "361921444@qq.com"
    }
  ],
  "license": "MIT",
  "autoload": {
